    "@nrwl/jest": "15.2.1",
    "@nrwl/js": "15.2.1",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/nx-plugin": "15.2.1",
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@types/fs-extra": "^9.0.13",
    "@types/is-ci": "^2.0.0",
    "@types/jest": "28.1.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "16.11.7",
    "@types/tmp": "^0.2.3",
