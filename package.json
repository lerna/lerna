    "@types/npmcli__arborist": "^5.6.6",
    "@types/npmlog": "^7.0.0",
    "@types/pify": "^5.0.4",
    "@types/semver": "^7.5.8",
    "@types/signal-exit": "^3.0.4",
    "@types/ssri": "^7.1.5",
    "@types/strong-log-transformer": "^1.0.2",
