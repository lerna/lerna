{
  "name": "lerna-monorepo",
  "private": true,
  "description": "lerna dog-fooding lerna",
  "workspaces": [
    "core/*",
    "packages/lerna",
    "packages/legacy-structure/commands/*"
  ],
  "volta": {
    "node": "16.15.0",
    "npm": "8.12.1",
    "yarn": "1.22.18"
  },
  "engines": {
    "node": "^14.15.0 || >=16.0.0"
  },
  "scripts": {
    "prerelease": "npm test && npm run integration",
    "release": "env-cmd lerna version",
    "ci:test:unit": "nx run-many -t test --maxWorkers=2 --ci",
    "ci:test:integration": "nx integration integration --maxWorkers=2 --ci",
    "ci:windows": "nx run-many -t test --ci --maxWorkers=2 && npm run integration -- --ci",
    "build": "scripts/build.sh",
    "fix": "npm run lint -- --fix",
    "format:check": "prettier --check '**/*.{js,ts,tsx,css,yml,json}'",
    "format:write": "prettier --write '**/*.{js,ts,tsx,css,yml,json}'",
    "integration": "nx integration integration --maxWorkers=2",
    "lint": "nx run-many -t lint",
    "pretest": "npm run lint",
    "test": "nx run-many -t test",
    "e2e-start-local-registry": "node ./scripts/e2e-start-local-registry.js",
    "e2e-build-package-publish": "ts-node -P ./scripts/tsconfig.e2e.json ./scripts/e2e-build-package-publish",
    "lerna-release": "nx run-many -t build --projects=legacy-structure-commands-version,legacy-structure-commands-publish && ts-node -P ./scripts/tsconfig.scripts.json ./scripts/lerna-release",
    "local-registry": "./scripts/local-registry.sh",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lerna/lerna.git"
  },
  "license": "MIT",
  "homepage": "https://lerna.js.org",
  "dependencies": {
    "@lerna/version": "file:packages/legacy-structure/commands/version",
