{
  "presets": [
    ["env", {
      "targets": {
        "node": 4
      }
    }]
  ],
  "env": {
    "test": {
      "presets": [
        ["env", {
          "exclude": ["transform-async-to-generator", "transform-regenerator"],
          "targets": {
            "node": "current"
          }
        }]
      ],
      "plugins": [
        ["fast-async", {
          "compiler": {
            "noRuntime": true
          }
        }]
      ]
    }
  }
}
