{
  "name": "legacy-structure-commands-exec",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/legacy-structure/commands/exec",
  "projectType": "application",
  "targets": {
    "build": {
      "dependsOn": ["compile"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/legacy-structure/commands/exec/dist",
        "parallel": false,
        "commands": ["rm -rf package.json"]
      }
    },
    "compile": {
      "executor": "@nrwl/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/legacy-structure/commands/exec/dist",
        "main": "packages/legacy-structure/commands/exec/src/index.ts",
        "tsConfig": "packages/legacy-structure/commands/exec/tsconfig.lib.json",
        "assets": [
          {
            "input": "libs/commands/exec",
            "glob": "README.md",
            "output": "../"
          }
        ],
        "thirdParty": false,
        "platform": "node",
        "format": ["cjs"],
        "esbuildOptions": {
          "external": ["*package.json"],
          "outExtension": {
            ".js": ".js"
          },
          "logOverride": {
            "commonjs-variable-in-esm": "silent"
          }
        },
        "bundle": false
      }
    }
  },
  "tags": []
}
