{
  "name": "@lerna/create",
  "version": "6.4.0",
  "description": "Create a new lerna-managed package",
  "keywords": [
    "lerna",
    "command"
  ],
  "homepage": "https://lerna.js.org",
  "license": "MIT",
  "author": {
    "name": "Daniel Stockman",
    "url": "https://github.com/evocateur"
  },
  "files": [
